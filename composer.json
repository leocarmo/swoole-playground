{
    "name": "leocarmo/swoole-boilerplate",
    "description": "Boilerplate for Swoole Http",
    "keywords": ["swoole", "http-swoole", "http"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Leonardo Carmo",
            "email": "leonardo.docarmo@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "nikic/fast-route": "^1.3",
        "vlucas/phpdotenv": "^3.4",
        "ext-json": "*"
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
